{
    "manifest_version": 2,
    "name": "Word meaning",
    "version": "0.1",
    "content_scripts": [
        {
            "matches":[
                "<all_urls>"
            ],
            "js": ["content.js"]
        }
    ],
    "background":{
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_icon": "icon.jpg",
        "default_popup": "index.html",
        "default_title": "Find A Word"
    }
}